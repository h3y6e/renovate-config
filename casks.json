{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^Casks/[^/]+\\.rb$/"
      ],
      "matchStringsStrategy": "combination",
      "matchStrings": [
        "version \"(?<currentValue>.*?)\"",
        "url \"https://github.com/(?<depName>[\\w-]+/[\\w-]+)/releases/download"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
